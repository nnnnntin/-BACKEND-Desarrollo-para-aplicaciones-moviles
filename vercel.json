{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger/(.*\\.(js|css|png))",
      "dest": "https://unpkg.com/swagger-ui-dist/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "https://unpkg.com/swagger-ui-dist/favicon-32x32.png"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
      }
    }
  ]
}