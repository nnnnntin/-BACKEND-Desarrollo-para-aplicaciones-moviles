{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger/(.*\\.(js|css|png))",
      "dest": "https://unpkg.com/swagger-ui-dist/$1"
    },
    {
      "src": "/swagger",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}